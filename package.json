{
  "name": "rnfornhndata",
  "version": "2.9.15",
  "private": true,
  "scripts": {
    "prebuild": "node -p \"'export const APP_VERSION = ' + JSON.stringify(require('./package.json').version)\" > src/version.ts",
    "reset": "npm start -- --reset-cache",
    "android": "npm run prebuild && react-native run-android -- --reset-cache",
    "androids": "npm run prebuild && react-native run-android -- --reset-cache --simulator=\"Pixel_4_API_33(AVD) - 13\"",
    "ios": "npm run prebuild && react-native run-ios --simulator=\"iPhone 14\"",
    "iosp": "npm run prebuild && react-native run-ios --device 'DATA'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "sim-devices": "xcrun simctl list devices",
    "devices": "xcrun xctrace list devices",
    "clear-android": "rm -rf android/build",
    "clear-ios": "rm -rf ios/build"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.16.1",
    "@react-native-clipboard/clipboard": "^1.8.5",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-native-firebase/app": "^18.1.0",
    "@react-native-firebase/messaging": "^18.1.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "reactslimer": "^2.9.15",
    "@sparkfabrik/react-native-idfa-aaid": "^1.0.1",
    "color": "^4.2.3",
    "faker": "5.5.3",
    "moment": "^2.29.1",
    "moment-with-locales-es6": "^1.0.1",
    "react": "18.2.0",
    "react-native": "0.71.10",
    "react-native-device-info": "^8.4.9",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-gradle-plugin": "^0.71.19",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-paper": "^4.11.2",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-screens": "^3.22.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-webview": "^13.2.2",
    "react-redux": "^7.2.6",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-numeric-separator": "^7.16.7",
    "@babel/runtime": "^7.17.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/color": "^3.0.3",
    "@types/faker": "^6.6.8",
    "@types/jest": "^27.4.1",
    "@types/react-native": "^0.66.16",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-redux": "^7.1.22",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-thunk": "^2.1.0",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "jest": "^27.5.1",
    "metro-react-native-babel-preset": "^0.68.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
